public class HW1 {	public static void main(String[] args) {		// TODO Auto-generated method stub				System.out.println("Question #1:");		double running = .0175 * 10 * 330;		double tcbr = running * 30; /*tcbr stands for total calories burned running*/		double basketball = .0175 * 8 * 330;		double tcbb = basketball * 30; /*tcbb stands for total calories burned playing basketball*/		double sleeping = .0175 * 1 * 330;		double tcbs = sleeping * 360; /*tcbs stands for total calories burned sleeping*/		double totalCalBurned = tcbs + tcbb + tcbr;		System.out.println("The total calories burned from activites is " + totalCalBurned + ".");				System.out.println();		System.out.println("Question #2:");		int tickets = 37;		System.out.println("At a price of 10 tickets per candy bar and 3 tickets per gumball, you are able to purchase 3 candy bars and 2 gumballs with " + tickets + " tickets.");				System.out.println();		System.out.println("Question #3:");		String emo = "I hate chocolate ice cream!";		System.out.println("The line of text to be changed is:");		System.out.println(emo);		String emo2 = emo.substring(6);		emo = emo.substring(0, 2) + "love" + emo2;		System.out.println("The new line now reads:");		System.out.println(emo);				System.out.println();		System.out.println("Question #4:");		System.out.println("jjjjjjjjjjjjj  aaaaa  vvv        vvv  aaaaa ");		System.out.println("     jjj      aa   aa  vvv      vvv  aa   aa");		System.out.println("     jjj      aaaaaaa   vvv    vvv   aaaaaaa");		System.out.println("     jjj      aa   aa    vvv  vvv    aa   aa");		System.out.println("jjj  jjj      aa   aa     vvvvvv     aa   aa");		System.out.println("jjjjjjjj      aa   aa      vvvv      aa   aa");	    			} }
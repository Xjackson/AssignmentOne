package AssignmentOne;import java.util.Scanner;public class QuestionEight {	public static void main(String[] args) {		// TODO Auto-generated method stub		Scanner scanObject = new Scanner(System.in);		System.out.println("Enter the price of the item");		System.out.println("(Item must cost between 25 cents and 1.00 dollar and be divisble by 5):"); 		int price = scanObject.nextInt();		int change = 100 - price;		int quarters = change / 25;		int qRemainder = change % 25;		int dimes = qRemainder / 10;		int dRemainder = qRemainder % 10;		int nickels = dRemainder / 5;		System.out.println("You bought an item for " + price + " cents at the vending machine.");		System.out.println("The machine gives you: "		+ quarters + " quarter(s), "		+ dimes + " dime(s), and "		+ nickels + " nickel(s).");									}}
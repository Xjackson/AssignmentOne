package AssignmentOne;import java.text.DecimalFormat;public class QuestionFive {	public static void main(String[] args) {		// TODO Auto-generated method stub		DecimalFormat numberFormatter = new DecimalFormat("0.00");		double gearSize = 100.00 ; //inches		double wheelDiameter = 100.00 * (1.00/12.00) * (1.00/5280.00);				double cadence = 90.00;		double Cadencepmi = 90.00 * 60.00;		double distance = wheelDiameter * Math.PI;		double speed = Cadencepmi * distance; 		String Speed = numberFormatter.format(speed);		System.out.print("With a diameter of 100 inches and a cadence of 90 rpm you will travel " + Speed +" miles per hour");	}}
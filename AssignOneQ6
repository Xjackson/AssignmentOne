package AssignmentOne;import java.util.Scanner;import java.text.DecimalFormat;public class QuestionSix {		public static void main(String[] arg) {						int n;			double r, guess;						Scanner scannerObject = new Scanner(System.in);			DecimalFormat decimalObject = new DecimalFormat("0.00");						System.out.println("This program estimates roughly the square root of a number");			System.out.print("Enter a positive value you want estimated: ");			n = scannerObject.nextInt();			guess = n/2.0;			r = (n/guess);			guess = (guess + r)/2.0;			r = (n/guess);			guess = (guess+r)/2.0;			r = (n/guess);			guess = (guess + r)/2.0;			r = (n/guess);			guess = (guess + r)/2.0;			r = (n/guess);			guess = (guess + r)/2.0;						System.out.print("The square root of " + n + " is approximately " + decimalObject.format(guess) );								}	}